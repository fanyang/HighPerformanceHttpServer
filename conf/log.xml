<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <File name="file" filename="log/error.log">
      <PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss.SSS} %-5level - %msg%n"/>
    </File>
  </appenders>
  <loggers>
    <root level="error">
      <appender-ref ref="file"/>
    </root>
  </loggers>
</configuration>